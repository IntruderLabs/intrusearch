{
	"template": {
		"settings" : {
			"plugins.index_state_management.rollover_alias": "app_citrix"
		},
		"mappings": {
			"properties": {
				"citrix": {
					"properties":{
						"VserverServiceIP": {
							"type": "ip"
						},
						"VserverServicePort": {
							"type": "long"
						},
						"SSLVPN_client_type": {
							"ignore_above": 1024,
							"type": "keyword"
						},
						"Http_resources_accessed": {
							"type": "long"
						},
						"NonHttp_services_accessed": {
							"type": "long"
						},
						"Total_TCP_connections": {
							"type": "long"
						},
						"Total_UDP_flows": {
							"type": "long"
						},
						"Total_policies_allowed": {
							"type": "long"
						},
						"Total_policies_denied": {
							"type": "long"
						},
						"Total_compressedbytes_send": {
							"type": "long"
						},
						"Total_compressedbytes_recv": {
							"type": "long"
						},
						"LogoutMethod": {
							"ignore_above": 1024,
							"type": "keyword"
						},
						"Monitor": {
							"ignore_above": 1024,
							"type": "keyword"
						},
						"Device": {
							"ignore_above": 1024,
							"type": "keyword"
						}
					}
				}
			}
		}
	}
}
